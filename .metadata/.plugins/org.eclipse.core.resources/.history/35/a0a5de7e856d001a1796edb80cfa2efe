package com.epam.TDD_JUNIT;

public class RemoveFirstTwo 
{
	String remove(String s)
	{
		s=s.toLowerCase();
		StringBuilder sb = new StringBuilder();
		char ch[]=s.toCharArray();
		for(int i=0;i<ch.length;i++)
		{
			if(i==0 && ch[i]=='A')
			{
				continue;
			}
			else if(i==1 && ch[i]=='A')
			{
				continue;
			}
			else
			{
				sb.append(ch[i]);
			}
		}
		return sb.toString();
	}

}
